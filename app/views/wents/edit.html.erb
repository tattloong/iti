<%= form_for @went do |f| %>
	<%= f.label :country %> <br />
	<%= f.text_field :country %> <br />

	<%= f.label :destination %> <br />
	<%= f.text_field :destination %> <br />

	<%= f.label :duration %> <br />
	<%= f.text_field :duration %> <br /><br />

	<%= f.label :activity %> 
	<%= f.text_field :activity %> 

	<%= f.label :address %> 
	<%= f.text_field :address %>

	<%= f.label :price %>
	<%= f.number_field :price, :class => 'text_field' %> <br />

	<%= f.label :description %>
	<%= f.text_area :description %> <br />

	<%= f.submit %>
<% end %>